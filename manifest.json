{
    "manifest_version": 3,
    "name": "Password Generator",
    "version": "1.0",
    "description": "Generate random passwords",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "padlock16x16.png",
            "48": "padlock48x48.png",
            "128": "padlock.png"
        }
    },
    "permissions": [
        "clipboardWrite"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}